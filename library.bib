journal = {Heart Rhythm},
number = {2},
pages = {175--185},
publisher = {Elsevier},
title = {{Mechanism of origin of conduction disturbances in aging human atrial bundles: Experimental and model study}},
url = {http://www.sciencedirect.com/science?\_ob=GatewayURL\&\_origin=inwardhub\&\_urlversion=4\&\_method=citationSearch\&\_piikey=S154752710602145X\&\_version=1\&md5=e59d437401eeb5b6a2f150968301a878 papers2://publication/doi/10.1016/j.hrthm.2006.10.023},
