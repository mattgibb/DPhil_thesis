annote = {        Serial histology      },
author = {Burton, Rebecca A B and Plank, Gernot and Schneider, J\"{u}rgen E and Grau, Vicente and Ahammer, Helmut and Keeling, Stephen L and Lee, Jack and Smith, Nicolas P and Gavaghan, David and Trayanova, Natalia and Kohl, Peter},
journal = {Annals of the New York Academy of Sciences},
pages = {301--319},
title = {{Three-dimensional models of individual cardiac histoanatomy: tools and challenges}},
url = {http://www3.interscience.wiley.com/journal/120176036/abstract?CRETRY=1\&SRETRY=0 papers2://publication/doi/10.1196/annals.1380.023},
volume = {1080},
