@article{Xie2009,
annote = {The distribution of ventricular cell and tissue types is important in the initiation and maintenance of arrhythmia. For example, fibroblasts alter conduction velocity and refractoriness through several mechanisms. Traditionally, only cardiomyocyte decoupling was considered important, where fibroblasts act as passive insulating barriers, creating convoluted ‘zigzag’ conduction pathways and retarding propagation (3, 4). Recent experimental studies have documented that functional gap junctions form between fibroblasts and myocytes (8, 9, 10). Gap junctions affect propagation in three main ways.  Firstly, the conductivity facilitated by the junctions enhances propagation, allowing fibroblasts to transduce activation between otherwise unconnected myocytes (6, 14). Secondly and contrastingly, the junctions cause electrotonic loading; fibroblasts act as capacitors coupled to the myocytes, absorbing ionic currents and delaying myocyte activation (18, Xie). Finally, the coupling elevates myocyte resting membrane potential, causing conduction velocity first to increase and then to decrease with increasing fibroblast concentration and/or gap junction conductance, until finally conduction failure occurs (13, Xie). Fibroblasts also modulate myocyte behaviour via the release of paracrine factors [Cardiac fibroblast paracrine factors alter impulse conduction and ion channel expression of neonatal rat cardiomyocytes. Cardiovascular Research 2009;83:688–697]. A recent computational study $\backslash$cite\{Xie:2009\} suggests that this mechanism adds to the range, complexity and significance of the two cell types' mutual interaction, whilst conceding: "A second major limitation relates to simulating laminar sheet structures using a 2D model, which may obscure potentially important 3D transfiber effects of the laminar clefts. A real 3D structure is needed for further study, ideally modeled on a realistic representation of 3D cell distribution and coupling."},
author = {Xie, Y and Garfinkel, A and Camelliti, P and Kohl, P and Weiss, J N and Qu, Z},
journal = {Heart Rhythm},
publisher = {Elsevier},
title = {{Effects of fibroblast-myocyte coupling on cardiac conduction and vulnerability to reentry: A computational study}},
url = {papers2://publication/uuid/A29BA3D4-978C-4DBA-9A4C-20FBE3F89D10},
year = {2009}
}
